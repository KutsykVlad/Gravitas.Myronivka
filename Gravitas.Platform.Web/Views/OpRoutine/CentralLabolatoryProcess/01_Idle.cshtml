@using Gravitas.Model.DomainValue
@using Gravitas.Platform.Web.ViewModel
@using Gravitas.Platform.Web.ViewModel.Shared
@model Gravitas.Platform.Web.ViewModel.CentralLaboratoryProcess.IdleVm

@Html.HiddenFor(m => m.NodeId, new { @id = "nodeId" })

@{ Html.RenderAction(
       "GetRegistry",
       "TicketContainer",
       new {
           nodeId = Model.NodeId,
           registerType = TicketRegisterType.CentralLaboratory,
           detailActionLink = new ActionLinkVm {
               NodeId = Model.NodeId,
               ControllerName = "CentralLaboratoryProcessController",
               ActionName = "Idle"
           }
       });
}